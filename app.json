{
  "expo": {
    "name": "Siva Tools",
    "slug": "kitten-tricks",
    "version": "1.0.0",
    "description": "UI Kitten Demo Application",
    "githubUrl": "https://github.com/akveo/SivaTools",
    "ios": {
      "bundleIdentifier": "com.akveo.SivaTools",
      "appStoreUrl": "https://itunes.apple.com/us/app/kitten-tricks/id1246143230",
      "buildNumber": "3"
    },
    "android": {
      "package": "com.akveo.SivaTools",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.akveo.SivaTools",
      "versionCode": 1
    },
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./src/assets/images/image-app-icon.png",
    "splash": {
      "image": "src/assets/images/image-splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "src/assets/images/*",
      "src/assets/fonts",
      "src/layouts/**/assets/*"
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#F7F9FC"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
